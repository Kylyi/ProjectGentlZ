{
  "name": "gentl",
  "description": "Administrative tool.",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Jakub Kylian",
  "homepage": "https://none.cz",
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "productName": "Gentl",
    "win": {
      "target": "nsis",
      "icon": "logo.ico",
      "publisherName": "Jakub Kylian"
    },
    "linux": {
      "target": "deb",
      "maintainer": "Jakub Kylian"
    },
    "nsis": {
      "installerIcon": "logo.ico",
      "uninstallerIcon": "logo.ico"
    },
    "extraResources": [
      {
        "from": "./src/defaultSettings/",
        "to": "defaultSettings",
        "filter": [
          "**/*",
          "!**/databaseSettings.json"
        ]
      }
    ]
  },
  "dependencies": {
    "bcrypt": "^3.0.4",
    "blob-util": "^2.0.2",
    "csvjson": "^5.1.0",
    "dataframe-js": "^1.4.1",
    "devextreme": "^18.2",
    "devextreme-vue": "^18.2",
    "docxtemplater": "^3.9.7",
    "electron-online": "^1.0.0",
    "electron-updater": "^4.0.6",
    "element-ui": "^2.5.4",
    "iconv-lite": "^0.4.24",
    "jszip": "2.6.1",
    "keytar": "^4.4.1",
    "localStorage": "^1.0.4",
    "material-design-icons-iconfont": "^4.0.4",
    "moment": "^2.24.0",
    "msnodesqlv8": "^0.6.12",
    "mssql": "^4.1.0",
    "plotly.js": "^1.45.3",
    "polyonic-secure-pouch": "^1.0.3",
    "pouchdb": "^7.0.0",
    "pouchdb-adapter-http": "^7.0.0",
    "pouchdb-find": "^7.0.0",
    "pouchdb-replication-stream": "^1.2.9",
    "pouchdb-upsert": "^2.2.0",
    "pouchdb-upsert-bulk": "^1.0.2",
    "underscore": "^1.9.1",
    "username": "^4.1.0",
    "v-tooltip": "^2.0.0-rc.33",
    "vue": "^2.6.3",
    "vue-electron": "^1.0.6",
    "vue-multiselect": "^2.1.3",
    "vue-notification": "^1.3.15",
    "vue-router": "^3.0.2",
    "vue-shortkey": "^3.1.6",
    "vue2-dropzone": "^3.5.2",
    "vuedraggable": "^2.19.2",
    "vuetify": "^1.5.2",
    "vuetify-upload-button": "^1.2.2",
    "vuex": "^3.1.0",
    "xlsx": "^0.14.1",
    "xlsx-template": "^0.5.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.8.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "devextreme-intl": "^18.2.6",
    "electron": "^3.1.3",
    "electron-builder": "20.38.4",
    "electron-is-dev": "^1.0.1",
    "electron-webpack": "^2.6.2",
    "node-abi": "^2.7.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "vue-loader": "^15.6.2",
    "vue-template-compiler": "^2.6.3",
    "webpack": "4.28.4"
  }
}
